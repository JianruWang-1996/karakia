<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.history.history5.HistoryFragment5">


    <LinearLayout
        android:id="@+id/history5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp">
        <ImageView
            android:id="@+id/closing2"
            android:layout_width="match_parent"
            android:layout_height="320dp"
            android:padding="0dp"
            android:src="@drawable/closing2"
            android:contentDescription="@string/closing1"
            app:layout_constraintStart_toStartOf="@+id/toolbar2"/>

        <TextView
            android:id="@+id/text_5"
            android:layout_width="match_parent"
            android:layout_height="270dp"
            android:text="@string/closing2"
            android:textAlignment="center"
            android:textSize="30sp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="@+id/closing1" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>